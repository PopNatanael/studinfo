{% extends '@layout/default.html.twig' %}

{% block title %}Year{% endblock %}

{% block content %}

    <div class="row">
        <div class="col-lg-12">
            <div class="card">
                <div class="card-body">
                <h1>All Years</h1>
                    <ul>
                        {% for year in years %}
                            <b> Year: </b> {{ year.year }} <br>
                            <b> Status: </b> {{ year.status }} <br> </li>
                        {% endfor %}
                    </ul>
                </div>
            </div>
        </div>
    </div><!--/.row-->

{% endblock %}
